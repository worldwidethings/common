{
  "name": "wwt-common",
  "version": "2.0.0",
  "type": "module",
  "sideEffects": false,
  "description": "",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/shared/index.js",
      "require": "./dist/shared/index.cjs"
    },
    "./node": {
      "types": "./dist/types/index.node.d.ts",
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "./browser": {
      "types": "./dist/types/index.browser.d.ts",
      "import": "./dist/browser/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "rimraf dist && tsup && npm run types",
    "types": "npm run types:shared && npm run types:node && npm run types:browser",
    "types:shared": "tsc -p tsconfig.types.shared.json",
    "types:node": "tsc -p tsconfig.types.node.json",
    "types:browser": "tsc -p tsconfig.types.browser.json",
    "typecheck": "npm run -s typecheck:shared && npm run -s typecheck:node && npm run -s typecheck:browser",
    "typecheck:shared": "tsc -p tsconfig.base.json --noEmit",
    "typecheck:node": "tsc -p tsconfig.types.node.json --noEmit",
    "typecheck:browser": "tsc -p tsconfig.types.browser.json --noEmit",
    "test": "mocha test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-company/your-lib-name.git"
  },
  "keywords": [
    ""
  ],
  "author": "WorldWideThings GmbH",
  "bugs": {
    "url": "https://github.com/your-company/your-lib-name/issues"
  },
  "homepage": "https://github.com/your-company/your-lib-name#readme",
  "peerDependencies": {
    "knight-change": "^2.0.0",
    "knight-criteria": "^2.2.1",
    "knight-json": "^1.0.1",
    "knight-misfit": "^1.0.0",
    "knight-object-db": "^3.0.2"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "chai": "^5.3.1",
    "mocha": "^11.7.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "coderitter-api-rmc": "^2.0.0"
  }
}
